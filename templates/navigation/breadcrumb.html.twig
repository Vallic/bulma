{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 */
#}
{% if breadcrumb %}
  {% embed '@bulma_components/components/breadcrumb/breadcrumb.html.twig' with
    {
      items: breadcrumb
    } only
  %}
  {% endembed %}
{% endif %}
